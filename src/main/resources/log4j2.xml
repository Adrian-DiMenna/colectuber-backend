<Configuration>
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %t %level %c %msg %n" />
		</Console>
		<File name="File" filename="ColectuberBackendLogs.log">
			<PatternLayout pattern="%d %t %level %c %msg %n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="info">
			<AppenderRef ref="Console" />
			<AppenderRef ref="File" />
		</Root>
	</Loggers>
</Configuration>